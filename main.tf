export AWS_ACCESS_KEY_ID="123456789"
export AWS_SECRET_ACCESS_KEY="123456789"
export AWS_REGION="us-west-2"

terraform {
  required_providers {
    aws = {
      source  = "hashicorp/aws"
      version = "~> 4.0"
    }
  }
}

# Configure the AWS Provider
provider "aws" {
  region = "us-east-1"
}

# Create a VPC
resource "aws_vpc" "example" {
  cidr_block = "10.0.0.0/16"
}

